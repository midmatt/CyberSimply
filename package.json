{
  "name": "cybersafe-news",
  "version": "1.0.0",
  "type": "module",
  "main": "index.ts",
  "scripts": {
    "start": "expo start --dev-client --clear --lan",
    "start:tunnel": "expo start --dev-client --clear --tunnel",
    "start:lan": "expo start --dev-client --lan",
    "start:reset": "rm -rf .expo && expo start --dev-client --clear --lan",
    "ios:dev": "expo run:ios --device --configuration Debug",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "clear": "npx expo start --clear",
    "build:android": "npx expo build:android",
    "build:ios": "npx expo build:ios",
    "build:testflight": "./build-testflight.sh",
    "build:production": "./build-production.sh",
    "build:preview": "eas build --platform ios --profile preview",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "eject": "npx expo eject",
    "generate:bootsplash": "react-native-bootsplash generate ./assets/bootsplash_logo.png --project-type expo --background FFFFFF --logo-width 160"
  },
  "dependencies": {
    "@expo/ngrok": "^4.1.3",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.4.6",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.7",
    "@supabase/supabase-js": "^2.58.0",
    "expo": "~53.0.23",
    "expo-asset": "~11.1.7",
    "expo-constants": "^17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-device": "~7.1.4",
    "expo-file-system": "~18.1.11",
    "expo-font": "^13.3.2",
    "expo-image-picker": "^16.1.4",
    "expo-in-app-purchases": "^14.5.0",
    "expo-linear-gradient": "^14.1.5",
    "expo-linking": "^7.1.7",
    "expo-notifications": "^0.31.4",
    "expo-sharing": "^13.1.5",
    "expo-splash-screen": "^0.30.10",
    "expo-status-bar": "^2.2.3",
    "node-fetch": "^3.3.2",
    "openai": "^5.20.3",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-bootsplash": "^6.3.11",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-iap": "^12.15.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-vector-icons": "^10.3.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-community/cli": "^20.0.2",
    "@types/react": "~19.0.10",
    "typescript": "~5.8.3"
  },
  "private": true
}

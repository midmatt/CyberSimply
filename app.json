{
  "expo": {
    "name": "CyberSimply",
    "slug": "cybersafe-news",
    "version": "1.0.0",
    "entryPoint": "./index.js",
    "orientation": "portrait",
    "icon": "./assets/icons/ios-light.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "jsc",
    "scheme": "cybersimply",
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icons/ios-light.png",
      "darkIcon": "./assets/icons/ios-dark.png",
      "tintIcon": "./assets/icons/ios-tinted.png",
      "buildNumber": "7",
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-1846982089045102~4493578427"
      },
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "processing"
        ],
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      },
      "bundleIdentifier": "com.cybersimply.app",
      "inAppPurchases": true
    },
    "android": {
      "icon": "./assets/icons/ios-light.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png",
        "monochromeImage": "./assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-1846982089045102~4493578427"
      },
      "package": "com.cybersimply.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "notification": {
      "icon": "./assets/icons/ios-light.png",
      "color": "#FF6B35",
      "androidMode": "default",
      "androidCollapsedTitle": "CyberSimply"
    },
    "plugins": [
      "expo-dev-client",
      [
        "expo-splash-screen",
        {
          "resizeMode": "contain",
          "backgroundColor": "#FFFFFF",
          "image": "./assets/splash-light.png",
          "dark": {
            "backgroundColor": "#000000",
            "image": "./assets/splash-dark.png"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icons/ios-light.png",
          "color": "#FF6B35",
          "defaultChannel": "default"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "526c8a77-d747-4a06-a867-9d472d683675"
      },
      "owner": "matt_mid"
    }
  }
}
